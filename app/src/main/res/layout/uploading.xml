<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/uploading"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/uploading_ui">

    <TextView
        android:id="@+id/upload_id"
        android:layout_width="159dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="15dp"
        android:text="@string/upload_id"
        android:textAppearance="@style/upload_id"
        android:gravity="center_horizontal|top"
        />
    <TextView
        android:id="@+id/explain"
        android:layout_width="277dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="93dp"
        android:text="@string/explain"
        android:textAppearance="@style/explain"
        android:gravity="center_horizontal|top"
        />

    <View
        android:id="@+id/up_rectangle"
        android:layout_width="170dp"
        android:layout_height="310dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="211dp"
        android:background="@drawable/up_rectangle" />
    <RelativeLayout
        android:id="@+id/up_rectangle_1"
        android:layout_width="130dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="242dp">
        <ImageView
            android:layout_width="130dp"
            android:layout_height="120dp"
            android:src="@drawable/icon_clothes"
            />
    </RelativeLayout>
    <TextView
        android:id="@+id/look_logo"
        android:layout_width="59dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="77dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="399dp"
        android:text="@string/look_logo"
        android:textAppearance="@style/look_logo"
        android:gravity="top"
        />
    <TextView
        android:id="@+id/look_explain"
        android:layout_width="157dp"
        android:layout_height="13dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="28dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="447dp"
        android:text="@string/look_explain"
        android:textAppearance="@style/look_explain"
        android:gravity="center_horizontal|top"
        />

    <View
        android:id="@+id/up_rectangle2"
        android:layout_width="170dp"
        android:layout_height="310dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="202dp"
        android:layout_marginTop="211dp"
        android:background="@drawable/up_rectangle2" />

    <RelativeLayout
        android:id="@+id/up_rectangle_2"
        android:layout_width="130dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="225dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="242dp">
        <ImageView
            android:layout_width="130dp"
            android:layout_height="120dp"
            android:src="@drawable/icon_clothes"
            />
    </RelativeLayout>
    <TextView
        android:id="@+id/item_logo"
        android:layout_width="54dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="263dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="399dp"
        android:text="@string/item_logo"
        android:textAppearance="@style/item_logo"
        android:gravity="top"
        />
    <TextView
        android:id="@+id/item_explain"
        android:layout_width="164dp"
        android:layout_height="13dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="208dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="447dp"
        android:text="@string/item_explain"
        android:textAppearance="@style/item_explain"
        android:gravity="center_horizontal|top"
        />








</RelativeLayout>
